<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <JobDef>
    <defName>Nuzzle</defName>
    <driverClass>JobDriver_Nuzzle</driverClass>
    <reportString>nuzzling TargetA.</reportString>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

  <JobDef>
    <defName>Mate</defName>
    <driverClass>JobDriver_Mate</driverClass>
    <reportString>mating with TargetA.</reportString>
    <casualInterruptible>false</casualInterruptible>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>

  <JobDef>
    <defName>LayEgg</defName>
    <driverClass>JobDriver_LayEgg</driverClass>
    <reportString>laying egg.</reportString>
    <casualInterruptible>false</casualInterruptible>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>
  
  <JobDef>
    <defName>PredatorHunt</defName>
    <driverClass>JobDriver_PredatorHunt</driverClass>
    <reportString>hunting TargetA.</reportString>
    <casualInterruptible>false</casualInterruptible>
    <checkOverrideOnDamage>OnlyIfInstigatorNotJobTarget</checkOverrideOnDamage>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
    <neverFleeFromEnemies>true</neverFleeFromEnemies>
  </JobDef>

  <JobDef>
    <defName>Maintain</defName>
    <driverClass>JobDriver_Maintain</driverClass>
    <reportString>maintaining TargetA.</reportString>
    <casualInterruptible>false</casualInterruptible>
    <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
  </JobDef>
  
</Defs>
