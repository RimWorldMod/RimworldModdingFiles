<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RuleDef>
    <defName>BasePart</defName>
    <symbol>basePart_indoors</symbol>
    <resolvers>
      <li Class="SymbolResolver_BasePart_Indoors" />
    </resolvers>
  </RuleDef>

  <RuleDef>
    <defName>BasePart_Indoors_Division</defName>
    <symbol>basePart_indoors_division</symbol>
    <resolvers>
      <li Class="SymbolResolver_BasePart_Indoors_Division_Split">
        <minRectSize>(3, 3)</minRectSize>
      </li>
    </resolvers>
  </RuleDef>

  <RuleDef>
    <defName>BasePart_Indoors_Leaf</defName>
    <symbol>basePart_indoors_leaf</symbol>
    <resolvers>
      <li Class="SymbolResolver_BasePart_Indoors_Leaf_Barracks">
        <minRectSize>(3, 3)</minRectSize>
        <selectionWeight>1.25</selectionWeight>
      </li>
      <li Class="SymbolResolver_BasePart_Indoors_Leaf_WorshippedTerminal">
        <minRectSize>(5, 7)</minRectSize>
        <selectionWeight>1.25</selectionWeight>
      </li>
      <li Class="SymbolResolver_BasePart_Indoors_Leaf_Brewery">
        <minRectSize>(4, 4)</minRectSize>
        <selectionWeight>0.75</selectionWeight>
      </li>
      <li Class="SymbolResolver_BasePart_Indoors_Leaf_DiningRoom">
        <minRectSize>(5, 5)</minRectSize>
        <selectionWeight>1.25</selectionWeight>
      </li>
      <li Class="SymbolResolver_BasePart_Indoors_Leaf_BatteryRoom">
        <minRectSize>(4, 4)</minRectSize>
      </li>
      <li Class="SymbolResolver_BasePart_Indoors_Leaf_ThroneRoom" MayRequire="Ludeon.RimWorld.Royalty">
        <minRectSize>(3, 3)</minRectSize>
      </li>
    </resolvers>
  </RuleDef>

</Defs>
