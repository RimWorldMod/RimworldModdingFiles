<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RuleDef>
    <defName>BasePart_Outdoors</defName>
    <symbol>basePart_outdoors</symbol>
    <resolvers>
      <li Class="SymbolResolver_BasePart_Outdoors" />
    </resolvers>
  </RuleDef>

  <RuleDef>
    <defName>BasePart_Outdoors_Division</defName>
    <symbol>basePart_outdoors_division</symbol>
    <resolvers>
      <li Class="SymbolResolver_BasePart_Outdoors_Division_Grid">
        <minRectSize>(12, 12)</minRectSize>
      </li>
      <li Class="SymbolResolver_BasePart_Outdoors_Division_Split">
        <minRectSize>(3, 3)</minRectSize> 
      </li>
    </resolvers>
  </RuleDef>

  <RuleDef>
    <defName>BasePart_Outdoors_LeafPossiblyDecorated</defName>
    <symbol>basePart_outdoors_leafPossiblyDecorated</symbol>
    <resolvers>
      <li Class="SymbolResolver_BasePart_Outdoors_LeafPossiblyDecorated" />
    </resolvers>
  </RuleDef>

  <RuleDef>
    <defName>BasePart_Outdoors_LeafDecorated</defName>
    <symbol>basePart_outdoors_leafDecorated</symbol>
    <resolvers>
      <li Class="SymbolResolver_BasePart_Outdoors_LeafDecorated_EdgeStreet">
        <minRectSize>(3, 3)</minRectSize>
      </li>
      <li Class="SymbolResolver_BasePart_Outdoors_LeafDecorated_RandomInnerRect">
        <minRectSize>(3, 3)</minRectSize>
      </li>
    </resolvers>
  </RuleDef>

  <RuleDef>
    <defName>BasePart_Outdoors_Leaf</defName>
    <symbol>basePart_outdoors_leaf</symbol>
    <resolvers>
      <li Class="SymbolResolver_BasePart_Outdoors_Leaf_Empty">
        <selectionWeight>0.01</selectionWeight>
      </li>
      <li Class="SymbolResolver_BasePart_Outdoors_Leaf_Farm">
        <selectionWeight>0.65</selectionWeight>
      </li>
      <li Class="SymbolResolver_BasePart_Outdoors_Leaf_PowerPlant">
        <selectionWeight>0.3</selectionWeight>
      </li>
      <li Class="SymbolResolver_BasePart_Outdoors_Leaf_Building">
        <minRectSize>(4, 4)</minRectSize>
      </li>
      <li Class="SymbolResolver_BasePart_Outdoors_Leaf_LandingPad" MayRequire="Ludeon.RimWorld.Royalty">
        <minRectSize>(9, 9)</minRectSize>
      </li>
    </resolvers>
  </RuleDef>

</Defs>
