  <SoundDef>
    <defName>WindTurbine_Ambience</defName>
    <sustain>True</sustain>
    <context>MapOnly</context>
    <priorityMode>PrioritizeNearest</priorityMode>
    <maxVoices>1</maxVoices>
    <subSounds>
      <li>
        <muteWhenPaused>True</muteWhenPaused>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Electricity/WindTurbine_Ambience_Loop_01</clipPath>
          </li>
        </grains>
        <volumeRange>13</volumeRange>
        <distRange>10~20</distRange>
        <filters>
          <li Class="SoundFilterLowPass">
            <cutoffFrequency>50</cutoffFrequency>
            <lowpassResonaceQ>1</lowpassResonaceQ>
          </li>
        </filters>
        <paramMappings>
          <li>
            <inParam Class="SoundParamSource_External">
              <inParamName>PowerOutput</inParamName>
              <defaultValue>2000</defaultValue>
            </inParam>
            <outParam Class="SoundParamTarget_PropertyLowPass">
              <filterProperty>Cutoff</filterProperty>
            </outParam>
            <paramUpdateMode>Constant</paramUpdateMode>
            <curve>
              <points>
                <li>(0,500)</li>
                <li>(1,5000)</li>
              </points>
            </curve>
          </li>
          <li>
            <inParam Class="SoundParamSource_External">
              <inParamName>PowerOutput</inParamName>
              <defaultValue>1</defaultValue>
            </inParam>
            <outParam Class="SoundParamTarget_Pitch" />
            <paramUpdateMode>Constant</paramUpdateMode>
            <curve>
              <points>
                <li>(0,0.93)</li>
                <li>(1,1.07)</li>
              </points>
            </curve>
          </li>
        </paramMappings>
      </li>
    </subSounds>
  </SoundDef>

