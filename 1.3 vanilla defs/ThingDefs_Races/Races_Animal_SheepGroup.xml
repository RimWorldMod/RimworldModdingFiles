      <trainability>None</trainability>
      <lifeExpectancy>12</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>AnimalBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>AnimalJuvenile</def>
          <minAge>0.25</minAge>
        </li>
        <li>
          <def>AnimalAdult</def>
          <minAge>0.3333</minAge>
