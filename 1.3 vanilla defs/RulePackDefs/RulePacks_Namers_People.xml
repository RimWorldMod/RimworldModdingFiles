<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>NamerPersonCorunan</defName>
    <include>
      <li>TribalWordUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=2)->[WordTribal] [WordTribal]</li>
        <li>r_name->[WordTribal] '[AnimalBadass]' [WordTribal]</li>
        <li>r_name->[Color] [WordTribal]</li>
        <li>r_name->[WordTribal] [Animal]</li>
        <li>r_name->[Animal] [WordTribal]</li>
        <li>r_name->[Color] [Animal]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>EmpireNameUtility</defName>
    <rulePack>
      <rulesStrings>
        <li>WordImperial(p=4)->[SylB][SylB]</li>
        <li>WordImperial(p=2)->[SylB][SylB][SylB]</li>
        <li>WordImperial(p=0.2)->[SylB][SylB][SylB][SylB]</li>
      </rulesStrings>
      <rulesFiles>
        <li>ImperialNameFirstMale->Names/Imperial_First_Male</li>
        <li>ImperialNameFirstFemale->Names/Imperial_First_Female</li>
        <li>ImperialNameLast->Names/Imperial_Last</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

</Defs>
