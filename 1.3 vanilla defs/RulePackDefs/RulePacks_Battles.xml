<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RulePackDef>
    <defName>Battle_Solo</defName>
    <rulePack>
      <rulesStrings>
        <li>r_battlename->[PARTICIPANT1_nameDef]'s report</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>Battle_Duel</defName>
    <rulePack>
      <rulesStrings>
        <li>r_battlename->[PARTICIPANT1_nameDef] vs [PARTICIPANT2_nameDef]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>Battle_Internal</defName>
    <rulePack>
      <rulesStrings>
        <li>r_battlename->[FACTION1_name] internal conflict</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>Battle_War</defName>
    <rulePack>
      <rulesStrings>
        <li>r_battlename->[FACTION1_name] vs [FACTION2_name]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>Battle_Brawl</defName>
    <rulePack>
      <rulesStrings>
        <li>r_battlename->Free-for-all</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
</Defs>
