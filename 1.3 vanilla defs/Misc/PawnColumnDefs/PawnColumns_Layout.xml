<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <PawnColumnDef>
    <defName>GapTiny</defName>
    <workerClass>PawnColumnWorker_Gap</workerClass>
    <gap>4</gap>
  </PawnColumnDef>

  <PawnColumnDef>
    <defName>GapSmall</defName>
    <workerClass>PawnColumnWorker_Gap</workerClass>
    <gap>10</gap>
  </PawnColumnDef>

  <PawnColumnDef>
    <defName>Gap</defName>
    <workerClass>PawnColumnWorker_Gap</workerClass>
    <gap>17</gap>
  </PawnColumnDef>

  <PawnColumnDef>
    <defName>GapWide</defName>
    <workerClass>PawnColumnWorker_Gap</workerClass>
    <gap>26</gap>
  </PawnColumnDef>

  <!-- Consumes the remaining space after every other column is at its optimal width -->
  <PawnColumnDef>
    <defName>RemainingSpace</defName>
    <workerClass>PawnColumnWorker_RemainingSpace</workerClass>
    <widthPriority>-1000</widthPriority>
    <ignoreWhenCalculatingOptimalTableSize>true</ignoreWhenCalculatingOptimalTableSize>
  </PawnColumnDef>

</Defs>
